          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "login": {
                    "type": "string",
                    "example": "ugo-bertrand"
                  },
                  "password": {
                    "type": "string",
                    "example": "bipbip"
                  },
                  "email": {
                    "type": "string",
                    "example": "ugo.bertrand@epitech.eu"
                  },
                  "firstname": {
                    "type": "string",
                    "example": "ugo"
                  },
                  "lastname": {
                    "type": "string",
                    "example": "bertrand"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
